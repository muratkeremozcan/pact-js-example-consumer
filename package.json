{
  "name": "pact-js-example-consumer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "Murat Ozcan",
  "license": "ISC",
  "scripts": {
    "cy:open-local": "cypress open --e2e --browser chrome --config-file cypress/config/local.config.ts",
    "cy:run-local": "cypress run --e2e --browser chrome --config-file cypress/config/local.config.ts",
    "mock:server": ". ./scripts/env-setup.sh && npx mockoon-cli start --data ./mockoon/provider-mock.json --port ${PORT}",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|json)\"",
    "fix:format": "npm run prettier -- --write",
    "validate": "npm-run-all --parallel typecheck lint fix:format",
    "test": "jest --detectOpenHandles --verbose --silent --config jest.config.ts",
    "test:watch": "jest --watch --config jest.config.ts",
    "test:consumer": "rm -rf ./pacts && jest --config jest-pact.config.ts",
    "publish:pact": ". ./scripts/env-setup.sh && pact-broker publish ./pacts --consumer-app-version=$GITHUB_SHA --branch=$GITHUB_BRANCH --broker-base-url=$PACT_BROKER_BASE_URL --broker-token=$PACT_BROKER_TOKEN",
    "can:i:deploy:consumer": ". ./scripts/env-setup.sh && pact-broker can-i-deploy --pacticipant WebConsumer --version=$GITHUB_SHA --to-environment dev --retry-while-unknown=10",
    "record:consumer:deployment": ". ./scripts/env-setup.sh && if [ \"$GITHUB_BRANCH\" = \"main\" ]; then pact-broker record-deployment --pacticipant WebConsumer --version $GITHUB_SHA --environment $npm_config_env; fi",
    "typecheck": "tsc --noEmit -p tsconfig.json && tsc --noEmit -p tsconfig.jest.json",
    "lint": "eslint --ext=.js,.ts,.tsx --fix"
  },
  "devDependencies": {
    "@bahmutov/cy-api": "2.2.6",
    "@bahmutov/cypress-esbuild-preprocessor": "2.2.3",
    "@esbuild-plugins/node-globals-polyfill": "0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "0.2.2",
    "@faker-js/faker": "8.4.1",
    "@mockoon/cli": "8.4.0",
    "@pact-foundation/pact": "13.1.3",
    "@pact-foundation/pact-cli": "16.0.0",
    "@types/jest": "29.5.13",
    "@types/lodash": "4.17.7",
    "@types/node": "20.16.5",
    "@typescript-eslint/eslint-plugin": "8.6.0",
    "@typescript-eslint/parser": "8.6.0",
    "axios": "1.7.7",
    "cy-spok": "1.6.2",
    "cypress": "13.14.2",
    "cypress-data-session": "2.8.5",
    "cypress-each": "1.14.0",
    "cypress-map": "1.40.0",
    "dotenv": "16.4.5",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.3",
    "eslint-plugin-cypress": "3.5.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-implicit-dependencies": "1.1.1",
    "eslint-plugin-import": "2.30.0",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "lodash": "4.17.21",
    "nock": "^13.5.5",
    "npm-run-all2": "5.0.2",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "typescript": "5.6.2"
  }
}
