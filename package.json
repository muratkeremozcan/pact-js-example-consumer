{
  "name": "pact-js-example-consumer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "Murat Ozcan",
  "license": "ISC",
  "scripts": {
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|json)\"",
    "fix:format": "npm run prettier -- --write",
    "validate": "npm-run-all --parallel typecheck lint fix:format",
    "test:consumer": "jest --testMatch '**/*.pacttest.(ts)' --testTimeout=20000",
    "publish:pact": ". ./scripts/env-setup.sh && pact-broker publish ./pacts --consumer-app-version=$GITHUB_SHA --branch=$GITHUB_BRANCH --broker-base-url=$PACT_BROKER_BASE_URL --broker-token=$PACT_BROKER_TOKEN",
    "can:i:deploy:consumer": ". ./scripts/env-setup.sh && pact-broker can-i-deploy --pacticipant WebConsumer --version=$GITHUB_SHA --to-environment dev --retry-while-unknown=10",
    "record:consumer:deployment": ". ./scripts/env-setup.sh && if [ \"$GITHUB_BRANCH\" = \"main\" ]; then pact-broker record-deployment --pacticipant WebConsumer --version $GITHUB_SHA --environment dev; fi",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext=.js,.ts,.tsx --fix"
  },
  "devDependencies": {
    "@pact-foundation/pact": "12.1.0",
    "@types/jest": "29.5.12",
    "@types/node": "18.19.47",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "axios": "1.5.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-cypress": "3.5.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-implicit-dependencies": "1.1.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prettier": "5.2.1",
    "jest": "29.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "3.3.3",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.5.4"
  }
}
