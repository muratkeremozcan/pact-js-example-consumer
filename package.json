    "@pact-foundation/pact-cli": "16.0.3",
    "@types/jest": "29.5.14",
    "@types/lodash": "4.17.12",
    "@types/node": "20.17.1",
    "@typescript-eslint/eslint-plugin": "8.11.0",
    "@typescript-eslint/parser": "8.11.0",
    "axios": "1.7.7",
    "cy-spok": "1.6.2",
    "cypress": "13.15.1",
    "cypress-data-session": "2.8.5",
