{
  "name": "pact-js-example-consumer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "Murat Ozcan",
  "license": "ISC",
  "scripts": {
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|json)\"",
    "fix-format": "npm run prettier -- --write",
    "test:consumer": "jest src/consumer-contract.spec.js",
    "publish:pact": ". ./scripts/env-setup.sh && pact-broker publish ./pacts --consumer-app-version=$GITHUB_SHA --branch=$GITHUB_BRANCH --broker-base-url=$PACT_BROKER_BASE_URL --broker-token=$PACT_BROKER_TOKEN",
    "can:i:deploy:consumer": ". ./scripts/env-setup.sh && pact-broker can-i-deploy --pacticipant WebConsumer --version=$GITHUB_SHA --to-environment dev",
    "record:consumer:deployment": ". ./scripts/env-setup.sh && if [ \"$GITHUB_BRANCH\" = \"main\" ]; then pact-broker record-deployment --pacticipant WebConsumer --version $GITHUB_SHA --environment dev; fi"
  },
  "devDependencies": {
    "@pact-foundation/pact": "12.1.0",
    "axios": "1.5.1",
    "jest": "29.7.0",
    "prettier": "^3.3.3"
  }
}
